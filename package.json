  "license": "ISC",
  "dependencies": {
    "@dreamirl/de-plugin-gamescreen": "file:@dreamirl/de-plugin-gamescreen",
    "@dreamirl/dreamengine": "github:dreamirl/dreamengine"
  },
  "devDependencies": {
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-asset-copier": "^1.0.0"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "parcel html/browser.html -d dist/dev/",
    "build": "parcel build index.html -d dist/prod/",
    "dev:electron": "NODE_ENV=electron parcel index.html -d dist/dev-electron/",
    "build:electron": "NODE_ENV=electron parcel build index.html -d dist/prod-electron/",
    "win-dev:electron": "set NODE_ENV=electron&& parcel index.html -d dist/dev-electron/",
    "win-build:electron": "set NODE_ENV=electron&& parcel build index.html -d dist/prod-electron/"
  },
  "alias": {
    "Game": "./src/Game.js",
    "config": "./src/config.js",
    "DreamWorldControler": "./src/custom/DreamWorldControler.js",
    "dreamWorld": "./src/custom/dreamWorld.js",
    "Planet": "./src/custom/Planet.js",
    "Player": "./src/custom/Player.js",
    "homeWorld": "./src/custom/homeWorld.js",
    "inputs": "./src/data/inputs",
    "audios": "./src/data/audios",
    "dictionary": "./src/data/dictionary",
    "achievements": "./src/data/achievements",
    "images": "./src/data/images"
  },
  "assetsPath": "./assets/"
}
